{
  "name": "base-react-site",
  "version": "1.0.0",
  "description": "A base to fork all my React based websites from",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.ts",
    "build": "webpack --config webpack.prod.ts",
    "lint": "eslint . --fix --max-warnings=0",
    "lint:check": "eslint . --max-warnings=0",
    "prettify": "prettier \"./src/**/*.{js,jsx,mjs,ts,tsx,css,scss,sass}\" --write",
    "prettify:check": "prettier \"./src/**/*.{js,jsx,mjs,ts,tsx,css,scss,sass}\" --check",
    "types:check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuadschoep/base-react-site.git"
  },
  "keywords": [
    "node",
    "react",
    "javascript",
    "typescript",
    "fork",
    "template"
  ],
  "author": "Joshua Schoep",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuadschoep/base-react-site/issues"
  },
  "homepage": "https://github.com/joshuadschoep/base-react-site#readme",
  "dependencies": {
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.1.3",
    "babel-preset-airbnb": "^5.0.0",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.7",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "styles-loader": "^4.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
